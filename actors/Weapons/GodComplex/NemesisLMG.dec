ActiveSound "NemRune/IdleUV"
//Inventory.PickupSound "NemRune/PickupUV"
Inventory.PickupMessage "\c[w2]Nemesis Rune! Awaken Nemesis weapons. Also drains HEALTH/ARMOR to 500/500 while active."
States
	{
	Spawn:
		TNT1 A 0 A_PlaySound("NemRune/IdleUV",7,1.0,1)
		NMRZ ABC 4 Bright
		NMRZ DED 4 Bright 
		NMRZ CBA 4 Bright 
		Loop
	Pickup:
		TNT1 A 0
		TNT1 A 0 A_PlaySound("NemRune/PickupUV", 8)
		TNT1 A 0 A_PlaySound("NemRune/PickupLMG", 9)
		TNT1 A 0 A_GiveInventory("NemesisHealth", 1)
		TNT1 A 0 A_GiveInventory("NemesisArmor", 1)
		TNT1 A 0 A_Print("You founds some.... Your Eternity of Death Cyberdemon... LMG is Now Fast Firing")
		TNT1 A 0 A_GiveInventory("NemesisLMGFast")
		TNT1 A 1 A_StopSound(7)
		Stop
	}
}

Actor NemesisLMGFast : Inventory { Inventory.MaxAmount 1 }

Actor NemesisRuneLMGSpawnerII : PB_Annihilator Replaces PB_Annihilator
{
	DropItem "CyberdemonsMissileLauncher", 64
	Dropitem "NemesisRune_LMG", 64
	Dropitem "NemesisBFG", 64
	Dropitem "NemesisLMG", 64
}

Actor NemesisRuneLMGSpawnerII : PB_Cyberdemon Replaces PB_Cyberdemon
{
	DropItem "CyberdemonsMissileLauncher", 64
	Dropitem "NemesisRune_LMG", 64
	Dropitem "NemesisBFG", 64
	Dropitem "NemesisLMG", 64
}

ACTOR NemesisHealth : Health
{
	Game Doom
	SpawnID 152
	+COUNTITEM
	+FLOORCLIP
	+DONTGIB
	+INVENTORY.ALWAYSPICKUP
	Inventory.Amount 1200
	Inventory.MaxAmount 1200
	Inventory.PickupMessage ""
	Inventory.PickUpSound ""
	States
	{
	Spawn:
		TNT1 A 0
		Loop
	}
}

Actor NemesisArmor : BasicArmorBonus
{
	Game Doom
	SpawnID 22
	Radius 20
	Height 16
	Inventory.Pickupmessage "Picked up an armor bonus."
	Inventory.Icon "BON2D0"
	Armor.Savepercent 100
	Armor.Saveamount 1200
	Armor.Maxsaveamount 1200
	+COUNTITEM
	+FLOORCLIP
	+DONTGIB
	+INVENTORY.ALWAYSPICKUP
	Inventory.PickUpSound "items/bonus2"
	States
	{
	Spawn:
		TNT1 A 0
		Loop
	}
}