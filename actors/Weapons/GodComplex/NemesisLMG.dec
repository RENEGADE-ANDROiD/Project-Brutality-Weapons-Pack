  {
  Spawn:
	TNT1 A 0
	TNT1 A 0 A_SpawnItemEx("NemesisBFGProjectileTrail1",0,0,0,frandom(-1,1),frandom(-1,1),frandom(-1,1))
	CLBA D 1 A_Explode(10,60,0,0)
