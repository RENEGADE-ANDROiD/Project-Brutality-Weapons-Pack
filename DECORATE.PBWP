// Tokens
	// BD stuff for some weapons in Fire and Ice
ACTOR bd_Token : Inventory
{
    Inventory.MaxAmount 1
    +inventory.Undroppable
    -inventory.Invbar
    +NONETID
    +INVISIBLE
    +NOCLIP
    +NOTARGET
    +NOBLOCKMAP
    +NOINTERACTION
    Radius 0
    Height 0
    Alpha 0.0
    Mass 0
    RenderStyle "None"
}
ACTOR BDWeaponACtion : bd_Token {}
ACTOR InAction : bd_Token {}
	// IN thingy
ACTOR IsUsingAddictivePackTypeIN : Inventory { Inventory.MaxAmount 1 }

